{
    "env": {
        "commonjs": true,
        "node": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "arrow-body-style":"off",
      "operator-linebreak":"off",
      "strict":"off",
      "arrow-parens":"off",
      "global-require":"off",
      "lines-around-directive":"off",
      "no-underscore-dangle":"off"
    }
}
