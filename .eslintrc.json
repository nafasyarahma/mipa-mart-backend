{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "commonjs"
    },
    "rules": {
      "arrow-body-style":"off",
      "operator-linebreak":"off",
      "strict":"off",
      "arrow-parens":"off",
      "global-require":"off"
    }
}
