generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/mipa_mart"
}

model Category {
  id          String    @id @db.VarChar(255)
  name        String    @db.VarChar(255)
  description String?   @db.Text
}
